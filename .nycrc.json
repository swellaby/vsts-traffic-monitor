{
  "reporter": [
    "html",
    "lcov",
    "cobertura",
    "text",
    "text-summary"
  ],
  "cache": true,
  "all": false,
  "per-file": true,
  "lines": 100,
  "statements": 100,
  "functions": 100,
  "branches": 100,
  "check-coverage": true,
  "include": [
    "src/**/*"
  ],
  "exclude": [
    "src/interfaces/**/*"
  ],
  "extension": [
    ".ts"
  ],
  "report-dir": ".coverage/unit"
}
