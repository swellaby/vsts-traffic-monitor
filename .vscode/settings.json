{
  "files.exclude": {
    "**/*.js": { "when": "$(basename).ts" },
    "**/*.map": { "when": "$(basename).map" },
    "npm-debug.log": true,
    ".vsts-publish": true,
    ".vsix": true,
    ".coverage": true,
    ".testresults": true,
    ".nyc_output": true,
    "node_modules": true,
    ".taskkey": true
  },
  "files.associations": {
    "**/.azure-pipelines/**": "azure-pipelines"
  },
}
