{
  "name": "vsts-traffic-monitor",
  "description": "Monitors traffic on a VS Team Services account ",
  "version": "0.4.0-beta",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/swellaby/vsts-traffic-monitor",
    "type": "git"
  },
  "author": {
    "name": "swellaby",
    "email": "opensource@swellaby.com",
    "url": "http://swellaby.com"
  },
  "contributors": [
    {
      "name": "Caleb Cartwright",
      "url": "https://github.com/calebcartwright"
    }
  ],
  "scripts": {
    "build": "gulp clean tslint transpile eslint check-security enforce-code-coverage",
    "check-security": "gulp check-security",
    "clean": "gulp clean",
    "component-test": "gulp run-component-integration-tests",
    "enforce-code-coverage": "gulp enforce-code-coverage",
    "gulp": "gulp",
    "lint": "gulp tslint eslint",
    "show-component-integration-test-coverage": "gulp show-componengit stt-integration-test-coverage-report",
    "show-unit-test-coverage": "gulp show-unit-test-coverage-report",
    "test": "gulp enforce-code-coverage check-security",
    "test-offline": "gulp enforce-code-coverage",
    "transpile": "gulp transpile",
    "docker-build": "bash build.sh",
    "start": "node src/app.js",
    "inline": "node -e \"require('./src/task.js').run()\" ",
    "package-local-vsts-task": "gulp package-vsts-task-src package-vsts-task-files",
    "pack-up-local-vsts-task": "npm run package-local-vsts-task && cd .vsts-publish && npm prune --production && tfx build tasks upload --task-path .",
    "package-vsts-task": "npm prune --production && npm run package-local-vsts-task",
    "pack-up-vsts-task": "npm run package-vsts-task && npm run upload-vsts-task",
    "upload-vsts-task": "tfx build tasks upload --task-path .vsts-publish",
    "package-local-vsts-task-extension": "gulp package-vsts-task-extension-files",
    "pack-pub-local-vsts-task-extension": "npm run package-local-vsts-task-extension && cd .vsts-publish && npm prune --production && tfx "
  },
  "pre-commit": [
    "build"
  ],
  "devDependencies": {
    "@types/chai": "^4.0.3",
    "@types/express": "^4.0.36",
    "@types/mocha": "^2.2.41",
    "@types/moment": "^2.13.0",
    "@types/nconf": "0.0.34",
    "@types/node": "^8.0.24",
    "@types/request": "2.0.1",
    "@types/sinon": "^2.3.3",
    "chai": "^4.1.1",
    "del": "^3.0.0",
    "eslint": "^4.5.0",
    "gulp": "^3.9.1",
    "gulp-eslint": "^4.0.0",
    "gulp-istanbul": "^1.1.2",
    "gulp-mocha": "^3.0.1",
    "gulp-nsp": "^2.4.2",
    "gulp-open": "^2.0.0",
    "gulp-sourcemaps": "^2.6.1",
    "gulp-tslint": "^8.1.2",
    "gulp-typescript": "^3.2.1",
    "istanbul": "^0.4.5",
    "mocha": "^3.5.0",
    "mocha-multi": "^0.11.0",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "require-dir": "^0.3.2",
    "sinon": "^3.2.1",    
    "tslint": "^5.6.0",
    "tslint-microsoft-contrib": "^5.0.1",
    "typescript": "^2.4.2"
  },
  "dependencies": {
    "express": "^4.15.4",
    "moment": "^2.18.1",
    "nconf": "^0.8.4",
    "range_check": "^1.4.0",
    "request": "^2.81.0",
    "tslib": "^1.7.1",
    "vsts-task-lib": "^2.0.7"
  }
}
