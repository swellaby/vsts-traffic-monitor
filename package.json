{
  "name": "vsts-traffic-monitor",
  "description": "Monitors traffic on a VS Team Services account ",
  "version": "0.8.24",
  "license": "MIT",
  "repository": {
    "url": "https://github.com/swellaby/vsts-traffic-monitor",
    "type": "git"
  },
  "author": {
    "name": "swellaby",
    "email": "opensource@swellaby.com",
    "url": "http://swellaby.com"
  },
  "contributors": [
    {
      "name": "Caleb Cartwright",
      "url": "https://github.com/calebcartwright"
    }
  ],
  "scripts": {
    "build": "gulp clean tslint transpile eslint check-security enforce-code-coverage",
    "check-security": "gulp check-security",
    "clean": "gulp clean",
    "component-test": "gulp run-component-integration-tests",
    "enforce-code-coverage": "gulp enforce-code-coverage",
    "gulp": "gulp",
    "lint": "gulp tslint eslint",
    "show-component-integration-test-coverage": "gulp show-component-integration-test-coverage-report",
    "show-unit-test-coverage": "gulp show-unit-test-coverage-report",
    "test": "gulp enforce-code-coverage check-security",
    "test-offline": "gulp enforce-code-coverage",
    "transpile": "gulp transpile",
    "docker-build": "bash build.sh",
    "start": "node src/app.js",
    "inline": "node -e \"require('./src/task.js').run()\" ",
    "tfx-login": "tfx login",
    "package-vsts-task": "gulp package-vsts-task-src package-vsts-task-files",
    "upload-vsts-task": "tfx build tasks upload --task-path .vsts-publish/task",
    "pack-up-vsts-task": "npm run package-vsts-task && npm run upload-vsts-task",
    "bundle-vsts-task-ext-files": "gulp package-vsts-task-extension-files",
    "package-vsts-task-extension": "gulp package-vsts-task-extension-files && cd .vsts-publish && tfx extension create",
    "publish-vsts-task-extension": "cd .vsts-publish && tfx extension publish",
    "pack-pub-vsts-task-extension": "gulp package-vsts-task-extension-files && cd .vsts-publish && tfx extension publish",
    "dev:reset": "rimraf .testresults .coverage && node -e \"require('rimraf').sync('node_modules');\" && npm run dev:setup",
    "dev:setup": "npm i && npm run build"
  },
  "pre-commit": [
    "build"
  ],
  "devDependencies": {
    "@swellaby/eslint-config": "^1.1.6",
    "@swellaby/tslint-config": "^1.1.4",
    "@types/chai": "^4.1.3",
    "@types/express": "^4.16.0",
    "@types/mocha": "^5.2.1",
    "@types/moment": "^2.13.0",
    "@types/nconf": "0.0.37",
    "@types/request": "2.47.0",
    "@types/sinon": "^5.0.1",
    "chai": "^4.1.2",
    "copy-node-modules": "^1.0.5",
    "del": "^3.0.0",
    "eslint": "^4.19.1",
    "gulp": "^3.9.1",
    "gulp-bump": "^3.1.1",
    "gulp-eslint": "^4.0.2",
    "gulp-istanbul": "^1.1.3",
    "gulp-mocha": "^3.0.1",
    "gulp-nsp": "^3.0.1",
    "gulp-open": "^3.0.1",
    "gulp-sourcemaps": "^2.6.4",
    "gulp-tslint": "^8.1.3",
    "gulp-typescript": "^4.0.2",
    "gulp-vsts-bump": "^1.0.7",
    "istanbul": "^0.4.5",
    "mocha": "^5.2.0",
    "mocha-multi": "^1.0.1",
    "path": "^0.12.7",
    "pre-commit": "^1.2.2",
    "require-dir": "^1.0.0",
    "rimraf": "^2.6.2",
    "sinon": "^6.0.0",
    "tfx-cli": "^0.5.10",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.1"
  },
  "dependencies": {
    "express": "^4.16.3",
    "moment": "^2.22.2",
    "nconf": "^0.10.0",
    "range_check": "^1.4.0",
    "request": "^2.87.0",
    "tslib": "^1.9.2",
    "vsts-task-lib": "^2.4.0"
  }
}
